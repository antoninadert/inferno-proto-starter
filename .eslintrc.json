{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "inferno/jsx-no-undef": [0],
        "inferno/no-unknown-property": [0]
    },
    "plugins": ["inferno", "jsx-a11y"],
    "extends": ["plugin:jsx-a11y/recommended", "plugin:inferno/recommended"]
}