{
  "name": "inferno-proto-starter",
  "author": "Antonin Adert",
  "license": "Apache 2.0",
  "private": true,
  "scripts": {
    "start": "meteor run"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "bcrypt": "^1.0.3",
    "core-js": "^2.4.1",
    "history": "^4.7.2",
    "inferno": "^3.9.0",
    "inferno-component": "^3.9.0",
    "inferno-server": "^3.9.0",
    "meteor-node-stubs": "~0.2.4",
    "universal-router": "^3.2.0",
    "viewmodel-react": "^2.4.1",
    "viewmodel-react-plugin": "^3.1.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "viewmodel-react-plugin",
        {
          "useInferno": true
        }
      ],
      "babel-plugin-syntax-jsx",
      [
        "transform-object-rest-spread"
      ],
      "transform-es2015-spread",
      [
        "babel-plugin-inferno",
        {
          "imports": true
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-plugin-inferno": "^3.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "inferno-devtools": "^3.9.0"
  }
}
